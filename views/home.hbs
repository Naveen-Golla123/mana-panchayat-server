    <style>
        @import url('https://fonts.googleapis.com/css2?family=Akshar:wght@500&display=swap');

        :root {
            --theme-primary: #FF5C35;
            --theme-secondary: #FEF4EA;
            --theme-dark: #213343;
            --theme-light: #F6F9FC;
            --theme-white: #FFFFFF;

            --theme-font-color: #2E475D;
            --theme-font-family: Lexend Deca, sans-serif;

            --theme-card-shadow: 0 0 11px #eaf0f6;

        }

        .theme-shadow {
            box-shadow: var(--theme-card-shadow);
        }

        .cardu {
            border-radius: .5rem;
            box-shadow: var(--theme-card-shadow);
            background-color: var(--theme-white);
            color: var(--theme-font-color);

        }





        /* Theme Nav Bar */
        .navbar-theme-dark a {
            color: var(--theme-light) !important;
        }

        .navbar-theme-light a {
            color: var(--theme-dark) !important;
        }


        /* Theme Background Colors */

        .bg-theme-primary {
            background-color: #FF5C35 !important;
            color: var(--theme-font-color) !important;
        }

        .bg-theme-secondary {
            background-color: #FEF4EA !important;
            color: var(--theme-font-color) !important;
        }

        .bg-theme-dark {
            background-color: #FEF4EA !important;
            color: var(--theme-light) !important;
        }


        /* Theme Text Colors */

        .text-theme-primary {
            color: var(--theme-primary) !important;
        }

        .text-theme-secondary {
            color: var(--theme-secondary) !important;
        }

        .text-theme-dark {
            color: var(--theme-dark) !important;
        }


        body {

            font-family: var(--theme-font-family);
            color: var(--theme-font-color);
            background-color: var(--theme-light);
        }

        body p {
            font-size: 1.05rem;
        }


        /* Login Page  */

        .main-container {
            height: 100vh;
        }

        .login-container {
            width: 30rem;
        }

        .cursor-point {
            cursor: pointer;
        }


        .card {
            background-image: url('https://images.unsplash.com/photo-1500622944204-b135684e99fd?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=2922&q=80');
            background-size: cover;
        }


        #imgContainer {
            max-height: 25rem;
            aspect-ratio: 16/9;
        }

        .ad-block {
            width: auto;
            height: auto;
            aspect-ratio: 6.4/1;
            background-color: #E3F6FF;

            display: flex;
            justify-content: center;
            align-items: center;

            font-weight: bold;
            font-size: 2rem;
            color: cadetblue;

        }

        .tags-container ul {
            padding-inline-start: 0px;
        }

        .tags-container ul li {
            list-style: none;
            display: inline-block;
            border: .1px solid #4D727E;
            border-radius: .1rem;
            padding-inline: .5rem;
            margin-inline-end: .2rem;
            margin-top: .5rem;
            background-color: #EEEEEE;
            color: #1F4E5F;

        }

        .tags-container ul li:nth-child(1) {
            background-color: black;
            color: white;
        }

        .share-container ul {
            padding-inline-start: 0px;
        }

        .share-container ul li {
            list-style: none;
            display: inline-block;
            border: .1px solid #4D727E;
            border-radius: .1rem;
            padding-inline: .8rem;
            margin-inline-end: .2rem;
            margin-top: .5rem;
            background-color: #EEEEEE;
            color: #1F4E5F;

        }

        .share-container ul li:nth-child(1) {
            background-color: green;
            color: white;
        }



        .share_body ul {
            padding: 0;
            display: grid;
            grid-template-columns: repeat(4, 1fr);
        }

        .share_body ul li {
            max-width: 8rem;
            max-height: 8rem;
            padding: 1.3rem;
            margin: .8rem;
            list-style: none;
            text-align: center;
            /* background-color: aquamarine; */
            border: .1rem solid black;
            border-radius: .3rem;
            cursor: pointer;
        }

        .share_body ul li:hover {
            border: .1rem solid #674188;
            color: #FFFBF5;
            background-color: #674188;
        }

        .share_body ul li i {
            display: block;
            font-size: 1.5rem;
            margin-block: .8rem;
        }

        #newsTitle {
            font-size: 1.9rem;
            font-weight: bold;
        }

        #newsContent {
            font-size: 1.8rem;
            line-height: 2rem;
            word-spacing: 0.3rem;
            font-weight: 400;
        }


        .grid-box {
            display: grid;
            grid-template-columns: 2.5fr 1fr;
            gap: 2rem;
        }


        @media only screen and (max-width: 600px) {
            .grid-box {
                grid-template-columns: 1fr;
            }
        }

        .newsCardDescriptionText {
            white-space: nowrap;
            text-overflow: ellipsis;
            display: block;
            overflow: hidden
        }
    </style>

</head>

