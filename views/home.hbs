    <div class="container mt-5">

        <h4 class="font-weight-bold">Recent Posts</h4>

        <div id="allPosts">
            <div id="holder">
                {{#each data}}
                <div class="cardu border my-4">
                    <div class="row" id="news_${result[each].id}">
                        <div class="col-12 col-md-3">
                            <img class="w-100 h-100" src={{this.imgUrl}} alt="" srcset="">
                        </div>
                        <div class="col-12 col-md-9 newsCardDescription">
                            <div class="py-3 px-3">
                                <h5 class="font-weight-bold">{{this.title}}</h5>
                                <div class="mt-3 newsCardDescriptionText">
                                    {{{this.newsDescription}}}
                                </div>

                                <div class="d-flex justify-content-between mt-3 row">

                                    <a href={{readMeBlock this.id}} target="_blank" class="col-12 col-md-3 text-theme-primary p-2">Read
                                        More
                                        <i class="fa-solid fa-angles-right ml-1"></i>
                                    </a>

                                    <div class="d-flex justify-content-between justify-content-md-end ju col-12 col-md-9">
                                        <small class="text-secondary mr-3">
                                            <i
                                                class="fa-solid fa-location-dot mr-2 text-theme-primary"></i>{{this.location}}
                                        </small>
                                        <small class="text-secondary">
                                            <i
                                                class="fa-regular fa-clock mr-2 text-theme-primary"></i>{{this.createOnDate}}
                                        </small>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>

                {{/each}}
            </div>
        </div>

    </div>
